{
  "version": 2,
  "buildCommand": "chmod +x node_modules/.bin/vite && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/desktop_pc/(.*)",
      "dest": "/desktop_pc/$1"
    },
    {
      "src": "/planet/(.*)",
      "dest": "/planet/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
